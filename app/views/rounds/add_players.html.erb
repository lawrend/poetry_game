<%= @round.title %>

 <%= form_tag("/rounds/#{@round.id}", method: "patch") do %>
   <%= label_tag(:round, "Poets On This Joint") %><br>
   <%= collection_check_boxes(:round, :poem_user_ids, User.possible_players.delete_if {|player| player == current_user}, :id, :username) %><br>
   <%= label_tag(:round, "Title") %><br>
   <%= text_field(:round, :title) %><br>
   <%= hidden_field(:round, :active, :value => true) %>
   <%= submit_tag("Get the Party Started") %><br>
 <% end %>
or<br>

<%= link_to "Not yet, go home", player_path(current_user.id) %>
